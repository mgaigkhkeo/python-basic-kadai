        "# 消費税を加えた金額を計算する関数\n",
        "def calc_tax(price, tax_rate):\n",
        "    return price + (price * tax_rate / 100)\n",
        "\n",
        "# 第1引数：商品金額 110\n",
        "# 第2引数：消費税率 10%\n",
        "result = calc_tax(110, 10)\n",
        "\n",
        "print(result)\n"
