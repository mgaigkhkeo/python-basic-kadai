        "# 台形の各辺の長さ（cm）\n",
        "upper = 10\n",
        "lower = 20\n",
        "height = 5\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (upper + lower) * height / 2\n",
        "\n",
        "# 結果を出力\n",
        "print(f\"{area}cm²\")\n"
